{
  "name": "@thebookofeveryone/mapbox-geocoder",
  "version": "3.1.0",
  "description": "a mapbox geocoder component for autocomplete",
  "main": "index.js",
  "scripts": {
    "test": "tape test.js",
    "start": "watchify example/site.js -o example/bundle.js & st -nc",
    "babel": "babel --presets=es2015,react src/main.js -o index.js",
    "buildExample": "browserify -o example/bundle.js -e example/site.js -t babelify",
    "dist": "npm-run-all babel buildExample"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:thebookofeveryone/mapbox-geocoder.git"
  },
  "browserify": {
    "transform": [
      "babelify"
    ]
  },
  "keywords": [
    "react",
    "react-component",
    "geocoder",
    "flux",
    "mapbox",
    "geo"
  ],
  "author": "Tom MacWright",
  "license": "ISC",
  "devDependencies": {
    "babel-cli": "^6.23.0",
    "babel-core": "^6.23.1",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-react": "^6.23.0",
    "browserify": "^6.2.0",
    "parcel-bundler": "^1.12.4",
    "st": "^0.5.2",
    "tape": "^4.6.2",
    "watchify": "^2.1.0"
  },
  "dependencies": {
    "babelify": "^6.0.0",
    "react": "^16.4.0",
    "react-dom": "^16.4.0",
    "react-flip-move": "^2.9.1",
    "xhr": "^1.17.0"
  },
  "peerDependencies": {
    "react": "16.x",
    "react-dom": "16.x"
  }
}
